{{extend 'layout.html'}}

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
  	{{for student in students:}}
	    <tr>
	      <td>{{=student['id']}}</td>
	      <td>{{=student['first_name']}}</td>
	      <td>{{=student['email']}}</td>
	      <td><a href="delete?id={{=student['id']}}">delete</a></td>
	      <td></td>
	    </tr>
    {{pass}}
  </tbody>
</table>
